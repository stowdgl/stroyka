<?php

class Order {

    protected $orderID;
    protected $receiverID;
    protected $giverID;
    protected $orderDate;
    protected $storageID;
    protected $objectID;
    protected $commit;
    protected $requestedUnit;
    protected $measure;
    protected $quantity;

}


abstract public function getFullOrderList();


class Orderline extends Order{
    $orderlineid;
    $orderlinenamezakaz;
    $orderlinekolishestvo;
    $orderlinesumma;
    $orderlinedate;
}
